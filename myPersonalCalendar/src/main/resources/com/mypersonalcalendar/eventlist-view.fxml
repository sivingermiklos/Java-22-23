<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="450.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mypersonalcalendar.EventListController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
      <HBox prefHeight="33.0" prefWidth="450.0">
         <children>
            <Label text="Eseményeim">
               <font>
                  <Font name="Comic Sans MS" size="30.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="196.0" prefWidth="410.0">
          <TableView fx:id="myTableView" prefHeight="186.0" prefWidth="410.0">
              <columns>
              </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
          </TableView>
      </HBox>
    <HBox prefHeight="45.0" prefWidth="410.0">
        <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backToMain" prefHeight="42.0" prefWidth="53.0" text="← ">
                <font>
                    <Font name="Comic Sans MS" size="20.0" />
                </font>
            </Button>
        </children>
    </HBox>
</VBox>
